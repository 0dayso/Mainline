@model JinRi.Fx.Entity.SysAppTypeEntity

@{
    ViewBag.Title = "应用类型修改";
    Layout = "~/Views/Shared/_Layout.Edit.cshtml";
}
@section MainContent{
    <div class="portlet-body form-horizontal form-bordered form-row-stripped">
        <div class="row-fluid">

            <div class="control-group">
                <label class="control-label">编号：</label>
                <div class="controls">
                    @Model.AppTypeId
                </div>
            </div>
            <div class="control-group">
                <label class="control-label"><span class="required">*</span>类型名称：</label>
                <div class="controls">                    
                    @Html.TextBoxFor(m => m.TypeName, new { @class = "m-wrap" })
                    <span class="help-inline">@Html.ValidationMessageFor(m => m.TypeName)</span>
                </div>
            </div>
        </div>
    </div>
}